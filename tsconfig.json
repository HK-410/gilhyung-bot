{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".vercel/output",
    "rootDir": "api",
    "lib": ["ES2020"]
  },
  "include": [
    "api/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}